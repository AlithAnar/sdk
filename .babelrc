{
  "presets": ["es2015", "react", "stage-0", "flow"]
}

{
  "presets": [
    ["env", {
      "modules": false,
      "targets": { "browsers": ["last 2 versions", "ie >= 9"] }
    }],
    "react",
    "flow"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "env": {
    "production": {
      "presets": [
        "react-optimize"
      ]
    }
  }
}
